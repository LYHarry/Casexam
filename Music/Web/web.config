<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
    <authentication mode="Forms">
      <forms defaultUrl="User/Login.aspx" loginUrl="User/Login.aspx" />
    </authentication>
    <httpRuntime maxRequestLength="10000" executionTimeout="300"/>
    
    
  </system.web>
  <appSettings>
    <add key="StrConn" value="Data Source=.\SQLEXPRESS;Initial Catalog=Music;Integrated Security=True"/>
  </appSettings>
  <connectionStrings>
    <add name="StrConn" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=Music;Integrated Security=True"/>
  </connectionStrings>
</configuration>
